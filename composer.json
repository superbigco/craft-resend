{
    "name": "superbig/craft-resend",
    "description": "This plugin provides a Resend integration for Craft CMS.",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "support@superbig.com",
        "issues": "https://github.com/superbigco/craft-resend/issues?state=open",
        "source": "https://github.com/superbigco/craft-resend",
        "docs": "https://github.com/superbigco/craft-resend",
        "rss": "https://github.com/superbigco/craft-resend/releases.atom"
    },
    "require": {
        "php": ">=8.1",
        "craftcms/cms": "^4.9.0|^5.0.0",
        "symfony/resend-mailer": "^v7.1.0-BETA1"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "superbig\\resend\\": "src/"
        }
    },
    "extra": {
        "handle": "resend",
        "name": "Resend",
        "developer": "Superbig",
        "documentationUrl": "https://github.com/superbigco/craft-resend",
        "class": "superbig\\resend\\Resend"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

